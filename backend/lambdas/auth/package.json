    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "~5.5.0",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0"
  },
  "dependencies": {
    "aws-lambda": "^1.0.7",
